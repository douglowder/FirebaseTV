{
  "name": "firebasetest",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "start": "EXPO_TV=1 expo start",
    "android": "EXPO_TV=1 expo run:android",
    "ios": "EXPO_TV=1 expo run:ios",
    "web": "expo start --web",
    "prebuild": "DEBUG=expo:* EXPO_TV=1 expo prebuild --clean"
  },
  "dependencies": {
    "@react-native-firebase/app": "^21.3.0",
    "expo": "~51.0.31",
    "expo-build-properties": "~0.12.5",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "npm:react-native-tvos@~0.74.5-0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-tvos/config-tv": "^0.0.10",
    "@types/react": "~18.2.45",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true
}
